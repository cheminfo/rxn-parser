{
  "name": "rxn-parser",
  "version": "0.1.2",
  "description": "Parse the RXN format",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "cheminfo-build --entry src/index.js --root RXNParser",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "rollup -c",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/rxn-parser.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/rxn-parser/issues"
  },
  "homepage": "https://github.com/cheminfo/rxn-parser#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@vitest/coverage-v8": "^2.1.3",
    "cheminfo-build": "^1.2.0",
    "eslint": "^9.12.0",
    "eslint-config-cheminfo": "^12.0.1",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0"
  },
  "dependencies": {
    "cheminfo-types": "^1.8.0",
    "ensure-string": "^1.2.0",
    "vitest": "^2.1.3"
  }
}
