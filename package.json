  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "cheminfo-build": "^1.2.1",
    "eslint": "^9.33.0",
    "eslint-config-cheminfo": "^15.0.1",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "cheminfo-types": "^1.8.1",
    "ensure-string": "^1.2.0"
